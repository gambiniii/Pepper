<% chamados.forEach(chamado => { %>
  <% const ultimaAtt = chamado.atividade.length - 1 %>
  <% if(chamado.status === 'Em análise'){ %>

  <table class="table table-striped my-5">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Assunto</th>
        <th scope="col">Data Abertura</th>
        <th scope="col">Última Atualização</th>
        <th scope="col">STATUS</th>
      </tr>
    </thead>
    <tbody>


  
    <tr>
        <th scope="row" style="font-size: 14px;"> 
          <a href="/chamadosAtivos/tk/<%= chamado._id %>" style="text-decoration: none; color: #000;">
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-right-circle" viewBox="0 0 16 16" id="icone">
            <path fill-rule="evenodd" d="M1 8a7 7 0 1 0 14 0A7 7 0 0 0 1 8zm15 0A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM4.5 7.5a.5.5 0 0 0 0 1h5.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3a.5.5 0 0 0 0-.708l-3-3a.5.5 0 1 0-.708.708L10.293 7.5H4.5z"/>
          </svg>
          </a>
        <%= chamado._id %> 
      </th>
      <td> <%= chamado.assunto %> </td>
      <td> <%= chamado.dataAbertura %> </td>
      <td> <%= chamado.atividade[ultimaAtt].dataPostagem %> </td>
      <td> <%= chamado.status %> </td>
    </tr>
    <% }; %>
  <% }); %>







